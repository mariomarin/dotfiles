#![enable(implicit_some)]
(
    modkey: "Mod4",
    mousekey: "Mod4",
    workspaces: [],
    tags: [
        "\u{f113}",
        "\u{f0ac}",
        "\u{f120}",
        "\u{f02d}",
        "\u{f2c6}",
        "\u{f001}",
        "\u{f0e0}",
        "\u{f040}",
        "\u{f11b}",
    ],
    max_window_width: None,
    layouts: [
        "MainAndVertStack",
        "MainAndHorizontalStack",
        "MainAndDeck",
        "GridHorizontal",
        "EvenHorizontal",
        "EvenVertical",
        "Fibonacci",
        "LeftMain",
        "CenterMain",
        "CenterMainBalanced",
        "CenterMainFluid",
        "Monocle",
        "RightWiderLeftStack",
        "LeftWiderRightStack",
    ],
    layout_mode: Workspace,
    insert_behavior: Bottom,
    scratchpad: [
        (name: "Alacritty", value: "alacritty", x: 860, y: 390, width: 1000, height: 700),
    ],
    window_rules: [],
    disable_current_tag_swap: true,
    disable_tile_drag: false,
    disable_window_snap: true,
    focus_behaviour: Sloppy,
    focus_new_windows: true,
    single_window_border: true,
    sloppy_mouse_follows_focus: true,
    keybind: [
        // App launchers (cross-platform mnemonics)
        (command: Execute, value: "rofi -show drun", modifier: ["modkey"], key: "space"),
        (command: Execute, value: "alacritty", modifier: ["modkey", "Shift"], key: "Return"),
        (command: Execute, value: "alacritty", modifier: ["modkey"], key: "t"),
        (command: Execute, value: "firefox", modifier: ["modkey"], key: "b"),
        (command: Execute, value: "spotify", modifier: ["modkey"], key: "m"),
        (command: Execute, value: "obsidian", modifier: ["modkey"], key: "o"),
        (command: Execute, value: "slack", modifier: ["modkey"], key: "s"),
        (command: Execute, value: "ytfzf -D", modifier: ["modkey"], key: "y"),
        (command: Execute, value: "~/.config/rofi/power-menu.sh", modifier: ["modkey"], key: "p"),

        // Window mode (=-hold via kanata sends Ctrl+Alt)
        // i3-style: =+hjkl = focus, =+Shift+hjkl = move
        // Focus
        (command: FocusWorkspacePrevious, value: "", modifier: ["Control", "Alt"], key: "h"),
        (command: FocusWindowDown, value: "", modifier: ["Control", "Alt"], key: "j"),
        (command: FocusWindowUp, value: "", modifier: ["Control", "Alt"], key: "k"),
        (command: FocusWorkspaceNext, value: "", modifier: ["Control", "Alt"], key: "l"),
        // Move
        (command: MoveWindowToPreviousWorkspace, value: "", modifier: ["Control", "Alt", "Shift"], key: "h"),
        (command: MoveWindowDown, value: "", modifier: ["Control", "Alt", "Shift"], key: "j"),
        (command: MoveWindowUp, value: "", modifier: ["Control", "Alt", "Shift"], key: "k"),
        (command: MoveWindowToNextWorkspace, value: "", modifier: ["Control", "Alt", "Shift"], key: "l"),
        // Workspaces
        (command: GotoTag, value: "1", modifier: ["Control", "Alt"], key: "1"),
        (command: GotoTag, value: "2", modifier: ["Control", "Alt"], key: "2"),
        (command: GotoTag, value: "3", modifier: ["Control", "Alt"], key: "3"),
        (command: GotoTag, value: "4", modifier: ["Control", "Alt"], key: "4"),
        (command: GotoTag, value: "5", modifier: ["Control", "Alt"], key: "5"),
        (command: GotoTag, value: "6", modifier: ["Control", "Alt"], key: "6"),
        (command: GotoTag, value: "7", modifier: ["Control", "Alt"], key: "7"),
        (command: GotoTag, value: "8", modifier: ["Control", "Alt"], key: "8"),
        (command: GotoTag, value: "9", modifier: ["Control", "Alt"], key: "9"),
        // Move to workspace
        (command: MoveToTag, value: "1", modifier: ["Control", "Alt", "Shift"], key: "1"),
        (command: MoveToTag, value: "2", modifier: ["Control", "Alt", "Shift"], key: "2"),
        (command: MoveToTag, value: "3", modifier: ["Control", "Alt", "Shift"], key: "3"),
        (command: MoveToTag, value: "4", modifier: ["Control", "Alt", "Shift"], key: "4"),
        (command: MoveToTag, value: "5", modifier: ["Control", "Alt", "Shift"], key: "5"),
        (command: MoveToTag, value: "6", modifier: ["Control", "Alt", "Shift"], key: "6"),
        (command: MoveToTag, value: "7", modifier: ["Control", "Alt", "Shift"], key: "7"),
        (command: MoveToTag, value: "8", modifier: ["Control", "Alt", "Shift"], key: "8"),
        (command: MoveToTag, value: "9", modifier: ["Control", "Alt", "Shift"], key: "9"),
        // Other
        (command: ToggleFullScreen, value: "", modifier: ["Control", "Alt"], key: "f"),
        (command: CloseWindow, value: "", modifier: ["Control", "Alt"], key: "q"),
        (command: Execute, value: "alacritty", modifier: ["Control", "Alt"], key: "t"),
        (command: Execute, value: "alacritty", modifier: ["Control", "Alt"], key: "Return"),

        // Hyper bindings (Tab-hold = Ctrl+Alt+Super+Shift) for app launchers
        (command: Execute, value: "alacritty", modifier: ["modkey", "Control", "Alt", "Shift"], key: "t"),
        (command: Execute, value: "firefox", modifier: ["modkey", "Control", "Alt", "Shift"], key: "b"),
        (command: Execute, value: "spotify", modifier: ["modkey", "Control", "Alt", "Shift"], key: "m"),
        (command: Execute, value: "obsidian", modifier: ["modkey", "Control", "Alt", "Shift"], key: "o"),
        (command: Execute, value: "slack", modifier: ["modkey", "Control", "Alt", "Shift"], key: "s"),

        // Screenshots
        (command: Execute, value: "maim -s $HOME/Pictures/screenshots/\"region-$(date +%Y%m%d-%H%M%S).png\"", modifier: [], key: "Print"),
        (command: Execute, value: "maim -i $(xdotool getactivewindow) $HOME/Pictures/screenshots/\"window-$(date +%Y%m%d-%H%M%S).png\"", modifier: ["Shift"], key: "Print"),
        (command: Execute, value: "maim $HOME/Pictures/screenshots/\"monitor-$(date +%Y%m%d-%H%M%S).png\"", modifier: ["Control"], key: "Print"),
        (command: CloseWindow, value: "", modifier: ["modkey", "Shift"], key: "q"),
        (command: SoftReload, value: "", modifier: ["modkey", "Shift"], key: "r"),
        (command: Execute, value: "loginctl kill-session $XDG_SESSION_ID", modifier: ["modkey", "Shift"], key: "x"),
        (command: MoveToLastWorkspace, value: "", modifier: ["modkey", "Shift"], key: "w"),
        (command: SwapTags, value: "", modifier: ["modkey"], key: "w"),
        (command: MoveWindowUp, value: "", modifier: ["modkey", "Shift"], key: "k"),
        (command: MoveWindowDown, value: "", modifier: ["modkey", "Shift"], key: "j"),
        (command: MoveWindowTop, value: "", modifier: ["modkey"], key: "Return"),
        (command: FocusWindowUp, value: "", modifier: ["modkey"], key: "k"),
        (command: FocusWindowDown, value: "", modifier: ["modkey"], key: "j"),
        (command: NextLayout, value: "", modifier: ["modkey", "Control"], key: "k"),
        (command: PreviousLayout, value: "", modifier: ["modkey", "Control"], key: "j"),
        (command: SwapTags, value: "", modifier: ["modkey"], key: "Tab"),
        (command: FocusWorkspaceNext, value: "", modifier: ["modkey", "Alt"], key: "Tab"),
        (command: FocusWorkspacePrevious, value: "", modifier: ["modkey", "Alt", "Shift"], key: "Tab"),
        (command: FocusWorkspaceNext, value: "", modifier: ["modkey"], key: "l"),
        (command: FocusWorkspacePrevious, value: "", modifier: ["modkey"], key: "h"),
        (command: MoveWindowToNextWorkspace, value: "", modifier: ["modkey", "Shift"], key: "period"),
        (command: MoveWindowToPreviousWorkspace, value: "", modifier: ["modkey", "Shift"], key: "comma"),
        (command: MoveWindowUp, value: "", modifier: ["modkey", "Shift"], key: "Up"),
        (command: MoveWindowDown, value: "", modifier: ["modkey", "Shift"], key: "Down"),
        (command: FocusWindowUp, value: "", modifier: ["modkey"], key: "Up"),
        (command: FocusWindowDown, value: "", modifier: ["modkey"], key: "Down"),
        (command: NextLayout, value: "", modifier: ["modkey", "Control"], key: "Up"),
        (command: PreviousLayout, value: "", modifier: ["modkey", "Control"], key: "Down"),
        (command: FocusWorkspaceNext, value: "", modifier: ["modkey"], key: "Right"),
        (command: FocusWorkspacePrevious, value: "", modifier: ["modkey"], key: "Left"),
        (command: GotoTag, value: "1", modifier: ["modkey"], key: "1"),
        (command: GotoTag, value: "2", modifier: ["modkey"], key: "2"),
        (command: GotoTag, value: "3", modifier: ["modkey"], key: "3"),
        (command: GotoTag, value: "4", modifier: ["modkey"], key: "4"),
        (command: GotoTag, value: "5", modifier: ["modkey"], key: "5"),
        (command: GotoTag, value: "6", modifier: ["modkey"], key: "6"),
        (command: GotoTag, value: "7", modifier: ["modkey"], key: "7"),
        (command: GotoTag, value: "8", modifier: ["modkey"], key: "8"),
        (command: GotoTag, value: "9", modifier: ["modkey"], key: "9"),
        (command: MoveToTag, value: "1", modifier: ["modkey", "Shift"], key: "1"),
        (command: MoveToTag, value: "2", modifier: ["modkey", "Shift"], key: "2"),
        (command: MoveToTag, value: "3", modifier: ["modkey", "Shift"], key: "3"),
        (command: MoveToTag, value: "4", modifier: ["modkey", "Shift"], key: "4"),
        (command: MoveToTag, value: "5", modifier: ["modkey", "Shift"], key: "5"),
        (command: MoveToTag, value: "6", modifier: ["modkey", "Shift"], key: "6"),
        (command: MoveToTag, value: "7", modifier: ["modkey", "Shift"], key: "7"),
        (command: MoveToTag, value: "8", modifier: ["modkey", "Shift"], key: "8"),
        (command: MoveToTag, value: "9", modifier: ["modkey", "Shift"], key: "9"),
        (command: IncreaseMainWidth, value: "3", modifier: ["modkey", "Shift"], key: "Right"),
        (command: DecreaseMainWidth, value: "3", modifier: ["modkey", "Shift"], key: "Left"),
        (command: ToggleFullScreen, value: "", modifier: ["modkey"], key: "f"),
        (command: Execute, value: "xfce4-screensaver-command -l", modifier: ["modkey", "Shift"], key: "l"),
        (command: Execute, value: "pamixer -i 5", modifier: [], key: "XF86XK_AudioRaiseVolume"),
        (command: Execute, value: "pamixer -d 5", modifier: [], key: "XF86XK_AudioLowerVolume"),
        (command: Execute, value: "pactl set-sink-mute 0 toggle", modifier: [], key: "XF86XK_AudioMute"),
        (command: Execute, value: "pactl set-source-mute 1 toggle", modifier: [], key: "XF86XK_AudioMicMute"),
        (command: Execute, value: "light -U 5", modifier: [], key: "XF86XK_MonBrightnessDown"),
        (command: Execute, value: "light -A 5", modifier: [], key: "XF86XK_MonBrightnessUp"),
    ],
    state_path: None,
)
